{
  "compilerOptions": {
    "outDir": "Dist",
    "module": "es6",
    "target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "node_modules",
    "scripts",
    "acceptance-tests",
    "webpack",
  ],
  "types": [
    "typePatches"
  ]
}